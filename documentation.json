            "rawdescription": "\n",
            "type": "component",
            "sourceCode": "import { Component, EventEmitter, Input, Output } from '@angular/core';\r\n\r\nexport enum CommsButtonSizeEnum {\r\n  SMALL = 'small',\r\n  LARGE = 'large',\r\n}\r\n\r\n@Component({\r\n  selector: 'comms-button',\r\n  templateUrl: './comms-button.component.html',\r\n  styleUrls: ['./comms-button.component.scss'],\r\n})\r\nexport class CommsButtonComponent {\r\n  @Input() label?: string = 'Button';\r\n  @Input() icon?: string = 'pie_chart';\r\n  @Input() primary?: boolean;\r\n  @Input() size?: CommsButtonSizeEnum;\r\n\r\n  @Output() onClick = new EventEmitter<Event>();\r\n\r\n  public get classes(): string[] {\r\n    const mode = this.primary\r\n      ? 'storybook-button--primary'\r\n      : 'storybook-button--secondary';\r\n\r\n    return ['storybook-button', `storybook-button--${this.size}`, mode];\r\n  }\r\n}\r\n",
            "assetsDirs": [],
            "styleUrlsData": [
                {
                    "data": "@import '../../styles/global.scss';\r\n\r\n:host {\r\n  --button-background-color: var(--secondary);\r\n  --button-text-color: var(--secondary-text);\r\n  --button-border-radius: var(--spacing-xs);\r\n  --button-border-color: rgba(var(--secondary-text-rgb), 0.8);\r\n}\r\n\r\nbutton {\r\n  @include padding(\r\n    var(--spacing-sm),\r\n    var(--spacing-md),\r\n    var(--spacing-sm),\r\n    var(--spacing-md)\r\n  );\r\n\r\n  background: var(--button-background-color);\r\n  border: 1px solid var(--button-border-color);\r\n  border-radius: var(--button-border-radius);\r\n  color: var(--button-text-color);\r\n\r\n  &:hover {\r\n    --button-border-color: rgba(var(--secondary-text-rgb), 1);\r\n    cursor: pointer;\r\n  }\r\n\r\n  &.primary {\r\n    --button-background-color: var(--primary);\r\n    --button-text-color: var(--primary-text);\r\n    --button-border-color: transparent;\r\n\r\n    &:hover {\r\n      --button-background-color: rgba(var(--primary-rgb), 0.8);\r\n      cursor: pointer;\r\n    }\r\n  }\r\n\r\n  &.small {\r\n    @include padding(\r\n      var(--spacing-xs),\r\n      var(--spacing-xs),\r\n      var(--spacing-xs),\r\n      var(--spacing-xs)\r\n    );\r\n  }\r\n}\r\n",
                    "styleUrl": "./comms-button.component.scss"
                }
